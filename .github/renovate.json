{
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":semanticCommits",
    ":automergeTypes",
    ":disableDependencyDashboard",
    ":pinVersions",
    ":semanticCommitType(deps)",
    ":onlyNpm",
    ":label(dependencies)",
    "schedule:weekly"
  ],
  "baseBranches": [
    "main"
  ],
  "bumpVersion": "patch",

  "packageRules": [
    {
      "packageNames": [
        "node",
        "npm"
      ],
      "enabled": false
    },
    {
      "depTypeList": [
        "devDependencies"
      ],
      "semanticCommitType": "build"
    },
    {
      "matchPackageNames": [
        "electron"
      ],
      "separateMajorMinor": false
    }
  ]
}
